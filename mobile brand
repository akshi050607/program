#include <stdio.h>

struct mobile {
    char brand[20];
    char model[20];
    float price;
};

int main() {
    struct mobile m;

    printf("Enter Mobile Brand: ");
    scanf("%s", m.brand);

    printf("Enter Model Name: ");
    scanf("%s", m.model);

    printf("Enter Price: ");
    scanf("%f", &m.price);

    printf("\nMobile Details\n");
    printf("Brand: %s\nModel: %s\nPrice: %.2f\n",
           m.brand, m.model, m.price);

    return 0;
}
