#include <stdio.h>

int main() {
    int arr[10], n, i, newValue;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter new element: ");
    scanf("%d", &newValue);

    arr[n] = newValue;
    n++;

    printf("Updated Array:\n");
    for(i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
