#include <stdio.h>

int main() {
    int n, i, status, present = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        printf("Student %d (1=Present, 0=Absent): ", i);
        scanf("%d", &status);

        if (status == 1)
            present++;
    }

    printf("Number of Present Students = %d\n", present);
    return 0;
}
