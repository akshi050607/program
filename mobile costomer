#include <stdio.h>

struct customer {
    int id;
    char name[20];
    char mobile[15];
};

int main() {
    struct customer c[10];
    int n, i, searchId, found = 0;

    printf("Enter number of customers: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        scanf("%d %s %s", &c[i].id, c[i].name, c[i].mobile);
    }

    printf("Enter ID to search: ");
    scanf("%d", &searchId);

    for(i = 0; i < n; i++) {
        if(c[i].id == searchId) {
            printf("Name: %s Mobile: %s\n",
                   c[i].name, c[i].mobile);
            found = 1;
            break;
        }
    }

    if(!found)
        printf("Customer not found\n");

    return 0;
}
