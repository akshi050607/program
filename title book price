#include <stdio.h>

struct book {
    int id;
    char title[30];
    float price;
};

int main() {
    struct book b[10];
    int n, i;

    printf("Enter number of books: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("\nBook %d\n", i+1);
        scanf("%d %s %f", &b[i].id, b[i].title, &b[i].price);
    }

    printf("\nBook Details\n");
    for(i = 0; i < n; i++) {
        printf("ID: %d Title: %s Price: %.2f\n",
               b[i].id, b[i].title, b[i].price);
    }

    return 0;
}
